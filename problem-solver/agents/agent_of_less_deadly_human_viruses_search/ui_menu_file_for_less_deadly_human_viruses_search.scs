ui_menu_file_for_less_deadly_human_viruses_search
	<- 	ui_user_command_class_atom;
		ui_user_command_class_view_kb;
	=> nrel_main_idtf: 
		[Найти виды вирусов, которые вызывают менее смертельные заболевания человека, чем данный] (* <- lang_ru;; *);
		[Find less deadly virus species than this one] (* <- lang_en;; *);
	=> ui_nrel_command_template:
		[*
		question_less_deadly_virus _-> ._question_less_deadly_virus_instance
			(*
				_-> ui_arg_1;;
			*);;
		._question_less_deadly_virus_instance_<- question;;
		*];
	=> ui_nrel_command_lang_template: 
		[Запрос поиска для: $ui_arg_1] (* <- lang_ru;; *);
                [Search request for: $ui_arg_1] (* <- lang_en;; *);;
